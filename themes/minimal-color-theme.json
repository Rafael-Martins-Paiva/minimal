		"list.inactiveSelectionBackground": "#5D4C73",
		"button.background": "#3D27D1",
		"button.foreground": "#FFCEF2",
		"input.background": "#060522",
		"input.foreground": "#FFCEF2",
		"input.placeholderForeground": "#5D4C73",
		"dropdown.background": "#060522",
		"dropdown.foreground": "#FFCEF2",
		"pickerGroup.foreground": "#FFCEF2",
		"terminal.foreground": "#FFCEF2",
		"terminal.background": "#060522",
		"notification.background": "#060522",
		"notification.foreground": "#FFCEF2",
		"errorForeground": "#CA1734",
		"warningForeground": "#BF5500",
		"infoForeground": "#3D27D1"
