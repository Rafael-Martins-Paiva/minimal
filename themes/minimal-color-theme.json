		"button.border": "#060522",
		"list.activeSelectionBorder": "#060522",
		"list.focusBorder": "#060522",
		"tree.indentGuidesStroke": "#060522",
		"scrollbar.shadow": "#060522",

		"activityBar.background": "#060522",
		"sideBar.background": "#060522",
		"statusBar.background": "#060522",
